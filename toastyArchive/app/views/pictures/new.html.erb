<h1>Get more toasty!</h1>

<%= form_tag "/pictures" do %>
  <p>
    <%= label_tag :title %> <br>
    <%= text_field_tag :title %>
  </p>
  <p>
    <%= label_tag :date %> <br>
    <%= text_field_tag :date %>
  </p>
  <p>
    <%= label_tag :url %> <br>
    <%= text_field_tag :url %>
  </p>

  <p>
    <%= submit_tag "Save" %>
  </p>

<% end %>
