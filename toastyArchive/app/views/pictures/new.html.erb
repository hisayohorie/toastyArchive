<h1>Get more toasty!</h1>
<%= link_to "Back to Pictures", pictures_url %>
<%= form_for @picture do |f| %>
  <p>
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :date %> <br>
    <%= f.text_field :date %>
  </p>
  <p>
    <%= f.label :url %> <br>
    <%= f.text_field :url %>
  </p>

  <p>
    <%= f.submit "Save" %>
  </p>

<% end %>
